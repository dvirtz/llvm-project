{
  "name": "parquet-viewer",
  "displayName": "parquet-viewer",
  "description": "Views Apache Parquet files as JSON",
  "publisher": "dvirtz",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:parquet",
    "onFileSystem:file"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "parquet",
        "extensions": [
          ".parquet"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.viewParquetAsJson",
        "category": "Parquet",
        "title": "View as JSON"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}