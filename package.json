{
  "name": "parquet-viewer",
  "displayName": "parquet-viewer",
  "description": "Views Apache Parquet files as JSON",
  "publisher": "dvirtz",
  "icon": "images/ico128.png",
  "homepage": "https://github.com/dvirtz/vscode-parquet-viewer",
  "bugs": {
    "url": "https://github.com/dvirtz/vscode-parquet-viewer/issues",
    "email": "smcbreen@microsoft.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dvirtz/vscode-parquet-viewer.git"
  },
  "author": {
    "name": "Dvir Yitzchaki",
    "email": "dvirt@gmail.com",
    "url": "https://github.com/dvirtz"
  },
  "license": "MIT",
  "version": "0.1.5",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "parquet",
    "spark",
    "viewer"
  ],
  "activationEvents": [
    "onLanguage:parquet"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "parquet",
        "extensions": [
          ".parquet"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.viewParquetAsJson",
        "category": "Parquet",
        "title": "View as JSON"
      }
    ],
    "configuration": [
      {
        "title": "parquet-viewer",
        "properties": {
          "parquet-viewer.parquetToolsPath": {
            "type": "string",
            "default": "parquet-tools",
            "description": "Path to parquet-tools executable or jar"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "@types/vscode": "^1.30.0",
    "tslint": "^5.8.0",
    "typescript": "^3.1.4",
    "vscode-test": "^1.3.0",
    "mocha": "^6.1.4"
  }
}
